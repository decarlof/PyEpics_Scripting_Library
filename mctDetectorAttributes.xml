    <Attribute name="Delta_Theta"                     type="EPICS_PV" source="7bmb1:var:float2"             dbrtype="DBR_DOUBLE"/>
    <Attribute name="Speed_Theta"                     type="EPICS_PV" source="7bmb1:var:float1"             dbrtype="DBR_DOUBLE"/>
    <Attribute name="Magnification"                   type="EPICS_PV" source="7bmb1:userCalcOut2.C"         dbrtype="DBR_DOUBLE"/>

